config_version=4

_global_script_classes=[ {
"base": "Spatial",
"class": "Carte",
"language": "GDScript",
"path": "res://Scenes/Carte/Carte.gd"
}, {
"base": "Spatial",
"class": "Joueur",
"language": "GDScript",
"path": "res://Scenes/Joueur/Joueur.gd"
}, {
"base": "Node",
"class": "Pioche",
"language": "GDScript",
"path": "res://Scenes/Pioche/Pioche.gd"
} ]
_global_script_class_icons={
"Carte": "",
"Joueur": "",
"Pioche": ""
}

[application]

config/name="ARKED-Dixit-GODOT"
run/main_scene="res://Scenes/Menu/Menu.tscn"
config/icon="res://icon.png"
config/windows_native_icon="res://icone.ico"

[autoload]

Globals="*res://Scripts/Globals.gd"
Network="*res://Scripts/Network.gd"
R="*res://Scripts/R.gd"
Debug="*res://Scripts/Debug.gd"

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
